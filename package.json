{
  "name": "mycoshiro-linter",
  "version": "1.17.2",
  "description": "Enforces consistent markdown styling for Obsidian (https://obsidian.md). It can be used to format YAML tags, aliases, arrays, and metadata; footnotes; headings; spacing; math blocks; regular markdown contents like list, italics, and bold styles; and more with the use of custom rule options as well.",
  "packageManager": "yarn@3.6.1",
  "keywords": [
    "lint"
  ],
  "author": "Joel Van Eenwyk",
  "license": "MIT",
  "scripts": {
    "ci": "yarn install --frozen-lockfile",
    "compile": "yarn build && yarn docs && yarn lint && yarn test",
    "dev": "yarn ts-node build.ts",
    "build": "yarn ts-node build.ts production && yarn tsc jest.config.ts",
    "deploy": "cross-env-shell \"yarn shx --verbose cp -u ./dist/main.js manifest.json versions.json \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" \"",
    "docs": "yarn dev && yarn node ./dist/docs.js",
    "translate": "yarn build && node ./dist/translation-helper.js",
    "lint": "yarn eslint --fix **/*.ts",
    "check": "yarn ci && yarn compile && yarn lint && yarn test",
    "test": "yarn build && yarn test:clear && yarn test:base",
    "test:clear": "yarn ts-jest --clearCache",
    "test:base": "yarn ts-jest",
    "test:suite": "yarn ts-jest -t \"$1\""
  },
  "dependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.16.0",
    "@jest/types": "^29.6.1",
    "@popperjs/core": "^2.11.8",
    "@types/builtin-modules": "^3.1.1",
    "@types/diff": "^5.0.3",
    "@types/diff-match-patch": "^1.0.32",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@yarnpkg/sdks": "^3.0.0-rc.48",
    "babel-jest": "^29.6.2",
    "babel-plugin-import-glob": "^2.0.0",
    "builtin-modules": "^3.3.0",
    "cross-env": "^7.0.3",
    "diff-match-patch": "^1.0.5",
    "esbuild": "^0.19.1",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "js-yaml": "^4.1.0",
    "lint-staged": "^14.0.0",
    "loglevel": "^1.8.1",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm-footnote": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "micromark-extension-gfm-footnote": "^2.0.0",
    "micromark-extension-gfm-task-list-item": "^2.0.1",
    "micromark-extension-math": "^3.0.0",
    "micromark-util-combine-extensions": "^2.0.0",
    "moment": "^2.29.4",
    "moment-parseformat": "^4.0.0",
    "obsidian": "^1.4.0",
    "prettier": "^3.0.1",
    "quick-lru": "^6.1.1",
    "shx": "^0.3.4",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "unified-lint-rule": "^2.1.2",
    "unist-util-visit": "^5.0.0"
  }
}
