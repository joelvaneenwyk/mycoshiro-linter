{
  "name": "mycoshiro-linter",
  "version": "1.17.2",
  "description": "Enforces consistent markdown styling for Obsidian (https://obsidian.md). It can be used to format YAML tags, aliases, arrays, and metadata; footnotes; headings; spacing; math blocks; regular markdown contents like list, italics, and bold styles; and more with the use of custom rule options as well.",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "check": "tsc --skipLibCheck --noEmit",
    "deploy": "cross-env-shell \"yarn shx --verbose mkdir -p \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" && yarn shx --verbose cp -u src/styles.css dist/main.js manifest.json versions.json \"${PROJECT_CWD}/.obsidian/plugins/${npm_package_name}\" \"",
    "clean": "yarn shx --verbose rm src/**/*.js",
    "docs": "node dist/docs.js",
    "translate": "yarn build && node dist/translation-helper.js",
    "compile": "yarn build && yarn docs && yarn lint && yarn test",
    "lint": "eslint \"**/*.ts\" \"**/*.mjs\"",
    "lint:fix": "eslint \"**/*.ts\" \"**/*.mjs\" --fix",
    "test": "jest",
    "test:suite": "jest -t \"$1\"",
    "test:clear": "jest --clearCache",
    "release": "yarn build && yarn deploy"
  },
  "files": [
    "src/main.ts",
    "src/docs.ts",
    "src/translation-helper.ts",
    "styles.css",
    "main.js",
    "manifest.json",
    "versions.json"
  ],
  "keywords": [],
  "author": "Joel Van Eenwyk",
  "authorUrl": "https://github.com/joelvaneenwyk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/mycoshiro-linter.git"
  },
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "\\.js?(x)$": "babel-jest",
      "\\.ts?(x)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(unified-lint-rule|remark-util-from-markdown))"
    ],
    "testMatch": [
      "**/remove-yaml?(*.)+(spec|test).[t]s?(x)",
      "!**/__tests__/common.ts"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.23.3",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@babel/plugin-transform-private-methods": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "@jest/globals": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@popperjs/core": "^2.11.8",
    "@types/diff": "^5.0.8",
    "@types/diff-match-patch": "^1.0.36",
    "@types/jest": "^29.5.10",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/unist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-import-glob": "^2.0.0",
    "builtin-modules": "^3.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "diff-match-patch": "^1.0.5",
    "esbuild": "^0.19.7",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.54.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-json-files": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "import-glob": "^1.5.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.8.1",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-gfm-footnote": "^2.0.0",
    "mdast-util-gfm-task-list-item": "^2.0.0",
    "mdast-util-math": "^3.0.0",
    "micromark-extension-gfm-footnote": "^2.0.0",
    "micromark-extension-gfm-task-list-item": "^2.0.1",
    "micromark-extension-math": "^3.0.0",
    "micromark-util-combine-extensions": "^2.0.0",
    "moment": "^2.29.4",
    "moment-parseformat": "^4.0.0",
    "obsidian": "^1.4.11",
    "prettier": "^3.1.0",
    "quick-lru": "^7.0.0",
    "remark": "^15.0.1",
    "shx": "^0.3.4",
    "svelte": "^4.2.7",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "unified-lint-rule": "^2.1.2",
    "unist-util-visit": "^5.0.0",
    "vue-eslint-parser": "^9.3.2"
  }
}
